<Window x:Class="Przygotowanie.Window4"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Przygotowanie"
        mc:Ignorable="d"
        Title="Window4" Height="450" Width="800">
    <UniformGrid Rows="2" Columns="2">
        <Button Content="1" />
        <Button Content="2" />
        <Button Content="3" />
        <Button Content="4" />
    </UniformGrid>
<!--UniformGrid Podobny do Grid, ale dzieli przestrzeń na równe wiersze i kolumny. Kluczowe właściowści: Row i Columns = definiują liczbę wierszy i kolumn.-->
</Window>
