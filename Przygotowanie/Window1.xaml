<Window x:Class="Przygotowanie.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Przygotowanie"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <WrapPanel Orientation="Vertical">
        <Button Content="Przycisk 1" />
        <Button Content="Przycisk 2" />
        <Button Content="Przycisk 3" />
    </WrapPanel>
<!--WrapPanel układa elementy w jednym wierszu lub kolumnie, gdy zabraknie miejsca przechodzi do następnego rzędu/kolumny. Kluczowe właściwości:
    Orientation = określa kierunek(Horizontal lub Vertical), ItemWidth i ItemHeight = definuiją rozmiar elementów-->
</Window>
